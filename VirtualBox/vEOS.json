   "builders": [
      {
          "boot_wait": "1m30s",
          "boot_command" : [
            "admin<enter><wait>",
            "en<enter><wait>",
            "write erase<enter><wait>",
            "y<enter><wait>",
            "bash<enter><wait>",
            "echo 0011.2233.4455 > /mnt/flash/system_mac_address<enter><wait>",
            "exit<enter>",
            "reload now<enter>"
          ],
          "type": "virtualbox-veos",
